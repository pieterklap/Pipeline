##Start## "comet2pin"

if [[ $NAME != *"all"* ]]; then
    sed 's/output_pepxmlfile = ./output_pepxmlfile = 0/' "$PIDparam" > "$PIDparam""_percolator"
    sed -i 's/output_percolatorfile = ./output_percolatorfile = 1/' "$PIDparam""_percolator"
    PIDparam="$PIDparam""_percolator"
else
     sed -i 's/output_percolatorfile = ./output_percolatorfile = 1/' "$PIDparam"
fi

if [[ $output_suffix == "" ]]; then
    output_suffix=$(grep "output_suffix =" $PIDparam | awk '{print $3}')
fi
EXTENTION=$(echo "$PIDinput" |awk -F. '{print $NF}')
COMEToutput+=$(echo "$PIDinput" | sed "s/\.$EXTENTION/$output_suffix\.pin /g")

##End## "comet2pin"
