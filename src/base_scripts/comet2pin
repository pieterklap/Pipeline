##Start## "comet2pin"

sed 's/output_pepxmlfile = 1/output_pepxmlfile = 0/' "$PIDparam" > "$PIDparam""_percolator"
sed -i 's/output_percolatorfile = 0/output_percolatorfile = 1/' "$PIDparam""_percolator"

PIDparam="$PIDparam""_percolator"

EXTENTION=$(echo "$PIDinput" |awk -F. '{print $NF}')
COMEToutput=$(echo $PIDinput | sed "s/\.$EXTENTION/$MassTolerance\.pin/")
NAME_Cometoutput=$(echo $COMEToutput | awk -F/ '{print $NF}')

##End## "comet2pin"
